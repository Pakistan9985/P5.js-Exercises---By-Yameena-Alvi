function setup() {
  createCanvas(400, 400);
  background(255);
  noStroke();

  for (let i = 0; i < 50; i++) {
  
    let x = random(width);
    let y = random(height);
    
    let size = random(10, 80);
    
   
    fill(random(255), random(255), random(255), 150);
    
    ellipse(x, y, size, size);
  }
}
